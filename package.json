{
    "name": "obsidian-similar-notes",
    "version": "0.8.0",
    "description": "Recommends similar notes to the currently viewed note using semantic embeddings",
    "main": "main.js",
    "scripts": {
        "dev": "node esbuild.config.mjs",
        "build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
        "version": "node version-bump.mjs && git add manifest.json versions.json",
        "test": "node esbuild.config.mjs workers-only && vitest run",
        "test:watch": "vitest"
    },
    "keywords": [
        "obsidian",
        "plugin",
        "similar",
        "notes",
        "recommendation",
        "embeddings"
    ],
    "author": "Young Lee",
    "license": "MIT",
    "devDependencies": {
        "@testing-library/jest-dom": "^6.6.3",
        "@testing-library/react": "^14.0.0",
        "@testing-library/user-event": "^14.6.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.17.30",
        "@types/picomatch": "^4.0.2",
        "@types/react": "^18.2.0",
        "@types/react-dom": "^18.2.0",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "@vitejs/plugin-react": "^4.3.4",
        "builtin-modules": "^3.3.0",
        "esbuild": "^0.25.1",
        "esbuild-plugin-inline-worker": "^0.1.1",
        "jsdom": "^26.0.0",
        "ts-node": "^10.9.2",
        "tslib": "2.4.0",
        "typescript": "4.7.4",
        "vitest": "^3.0.8"
    },
    "dependencies": {
        "@huggingface/transformers": "^3.6.0",
        "@langchain/core": "^0.3.44",
        "@langchain/textsplitters": "^0.1.0",
        "@orama/orama": "^3.1.1",
        "@orama/plugin-data-persistence": "^3.1.1",
        "comlink": "^4.4.2",
        "esbuild-plugin-polyfill-node": "^0.3.0",
        "loglevel": "^1.9.2",
        "obsidian": "latest",
        "picomatch": "^4.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rxjs": "^7.8.2"
    }
}
